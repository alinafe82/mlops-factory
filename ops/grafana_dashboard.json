{
  "title": "MLOps Factory Inference",
  "panels": [
    {
      "type": "graph",
      "title": "Latency (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(api_request_latency_seconds_bucket[5m])) by (le))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Error Rate",
      "targets": [
        {
          "expr": "sum(rate(inference_errors_total[5m]))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Requests by Status",
      "targets": [
        {
          "expr": "sum(rate(api_requests_total[5m])) by (status)"
        }
      ]
    }
  ]
}